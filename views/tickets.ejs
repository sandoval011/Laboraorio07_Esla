<body>

	<!-- Main container -->
	<main class="full-box main-container">
		<!-- Nav lateral -->
		<section class="full-box nav-lateral">
			<div class="full-box nav-lateral-bg show-nav-lateral"></div>
			<div class="full-box nav-lateral-content">
				<figure class="full-box nav-lateral-avatar">
					<i class="far fa-times-circle show-nav-lateral"></i>
					<img src="/assets/avatar/esla.jpg" class="img-fluid" alt="Avatar">
					<figcaption class="roboto-medium text-center">
						ADMIN ESLA <br><small class="roboto-condensed-light">Web Developer</small>
					</figcaption>
				</figure>
				<div class="full-box nav-lateral-bar"></div>
				<nav class="full-box nav-lateral-menu">
					<ul>
						<li>
							<a href="home"><i class="fab fa-dashcube fa-fw"></i> &nbsp; Dashboard</a>
						</li>

						<li>
							<a href="#" class="nav-btn-submenu"><i class="fas fa-users fa-fw"></i> &nbsp; Hoteles <i
									class="fas fa-chevron-down"></i></a>
							<ul>
								<li>
									<a href="formulario"><i class="fas fa-plus fa-fw"></i> &nbsp; Agregar Hotel</a>
								</li>
								<li>
									<a href="client-list.html"><i class="fas fa-clipboard-list fa-fw"></i> &nbsp; Lista
										de Hoteles</a>
								</li>
								<li>
									<a href="client-search.html"><i class="fas fa-search fa-fw"></i> &nbsp; Buscar
										Hotel</a>
								</li>
							</ul>
						</li>

						<li>
							<a href="#" class="nav-btn-submenu"><i class="fas fa-pallet fa-fw"></i> &nbsp; Reservaciones
								<i class="fas fa-chevron-down"></i></a>
							<ul>
								<li>
									<a href="item-new.html"><i class="fas fa-plus fa-fw"></i> &nbsp; Agregar Reserva</a>
								</li>
								<li>
									<a href="item-list.html"><i class="fas fa-clipboard-list fa-fw"></i> &nbsp; Lista de
										Reservaciones</a>
								</li>
								<li>
									<a href="item-search.html"><i class="fas fa-search fa-fw"></i> &nbsp; Buscar
										Reservaciones</a>
								</li>
							</ul>
						</li>

						<li>
							<a href="#" class="nav-btn-submenu"><i class="fas fa-file-invoice-dollar fa-fw"></i> &nbsp;
								Empleados <i class="fas fa-chevron-down"></i></a>
							<ul>
								<li>
									<a href="customer_new.ejs"><i class="fas fa-plus fa-fw"></i> &nbsp; Nuevo
										Empleado</a>
								</li>
								<li>
									<a href="customer-list.ejs"><i class="fas fa-clipboard-list fa-fw"></i> &nbsp; Lista
										de Empleados</a>
								</li>
								<li>
									<a href="reservation-search.html"><i class="fas fa-search-dollar fa-fw"></i> &nbsp;
										Buscar Empleados</a>
								</li>
							</ul>
						</li>

						<li>
							<a href="#" class="nav-btn-submenu"><i class="fas  fa-user-secret fa-fw"></i> &nbsp; Tours
								<i class="fas fa-chevron-down"></i></a>
							<ul>
								<li>
									<a href="user-new.html"><i class="fas fa-plus fa-fw"></i> &nbsp; Nuevo Tours</a>
								</li>
								<li>
									<a href="user-list.html"><i class="fas fa-clipboard-list fa-fw"></i> &nbsp; Lista de
										Tours</a>
								</li>
								<li>
									<a href="user-search.html"><i class="fas fa-search fa-fw"></i> &nbsp; Buscar
										Tours</a>
								</li>
							</ul>
						</li>

						<li>
							<a href="#" class="nav-btn-submenu"><i class="fas  fa-user-secret fa-fw"></i> &nbsp; Fly <i
									class="fas fa-chevron-down"></i></a>
							<ul>
								<li>
									<a href="user-new.html"><i class="fas fa-plus fa-fw"></i> &nbsp; Nuevo Fly</a>
								</li>
								<li>
									<a href="user-list.html"><i class="fas fa-clipboard-list fa-fw"></i> &nbsp; Lista de
										Fly</a>
								</li>
								<li>
									<a href="user-search.html"><i class="fas fa-search fa-fw"></i> &nbsp; Buscar Fly</a>
								</li>
							</ul>
						</li>


						<li>
							<a href="#" class="nav-btn-submenu"><i class="fas  fa-user-secret fa-fw"></i> &nbsp; Ticket
								<i class="fas fa-chevron-down"></i></a>
							<ul>
								<li>
									<a href="user-new.html"><i class="fas fa-plus fa-fw"></i> &nbsp; Nuevo Ticket</a>
								</li>
								<li>
									<a href="user-list.html"><i class="fas fa-clipboard-list fa-fw"></i> &nbsp; Lista de
										Ticket</a>
								</li>
							</ul>
						</li>
					</ul>
				</nav>
			</div>
		</section>

		<!-- Page content -->
		<section class="full-box page-content">

			<!-- Page header -->
			<div class="full-box page-header">
				<h3 class="text-left">
					<i class="fas fa-clipboard-list fa-fw"></i> &nbsp; LISTA DE TICKETS
				</h3>
			</div>

			<div class="container-fluid">
				<ul class="full-box list-unstyled page-nav-tabs">
					<li>
						<a href="formularioTicket"><i class="fas fa-plus fa-fw"></i> &nbsp; AGREGAR TICKET</a>
					</li>
					<li>
						<a class="active" href="tickets"><i class="fas fa-clipboard-list fa-fw"></i> &nbsp; LISTA DE
							TICKETS</a>
					</li>
				</ul>
			</div>

			<!-- Content here-->
			<div class="container-fluid">
				<div class="table-responsive">
					<table class="table table-dark table-sm">
						<thead>
							<tr class="text-center roboto-medium">
								<th>#</th>
								<th>PRECIO</th>
								<th>TOUR</th>
								<th>VUELO</th>
								<th>CLIENTE</th>
								<th>FECHA DE SALIDA</th>
								<th>FECHA DE LLEGADA</th>
                                <th>FECHA DE COMPRA</th>
								<th>ACTUALIZAR</th>
								<th>ELIMINAR</th>
							</tr>
						</thead>
						<tbody>
							<% tickets.forEach((ticket, index)=> { %>
								<tr class="text-center">
									<td>
										<%= index + 1 %>
									</td>
									<td>
										<%= ticket.price %>
									</td>
									<td>
										<%= ticket.tour_id.name %>
									</td>
									<td>
										<%= ticket.fly_id.name %>
									</td>
									<td>
										<%= ticket.customer_id.full_name %>
									</td>
									<td>
										<%= ticket.departure_date %>
									</td>
									<td>
										<%= ticket.arrival_date %>
									</td>
                                    <td>
										<%= ticket.date_purchase %>
									</td>
									<td>
										<form action="/tickets/<%= ticket._id %>/editar" method="GET"
											style="display: inline;">
											<button type="submit" class="btn btn-success">
												<i class="fas fa-sync-alt"></i> Actualizar
											</button>
										</form>
									</td>
									<td>
										<form action="/tickets/<%= ticket._id %>/eliminar" method="POST"
											style="display: inline;">
											<button type="submit" class="btn btn-warning">
												<i class="far fa-trash-alt"></i> Eliminar
											</button>
										</form>
									</td>
								</tr>
								<% }); %>
						</tbody>
					</table>
				</div>
			</div>
		</section>
	</main>


	<!--=============================================
	=            Include JavaScript files           =
	==============================================-->
	<!-- jQuery V3.4.1 -->
	<script src="/js/jquery-3.4.1.min.js"></script>

	<!-- popper -->
	<script src="/js/popper.min.js"></script>

	<!-- Bootstrap V4.3 -->
	<script src="/js/bootstrap.min.js"></script>

	<!-- jQuery Custom Content Scroller V3.1.5 -->
	<script src="/js/jquery.mCustomScrollbar.concat.min.js"></script>

	<!-- Bootstrap Material Design V4.0 -->
	<script src="/js/bootstrap-material-design.min.js"></script>
	<script>$(document).ready(function () { $('body').bootstrapMaterialDesign(); });</script>

	<script src="/js/main.js"></script>
</body>

</html>